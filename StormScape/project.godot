config_version=5

[addons]

resources_spreadsheet_view/array_color_tint=100.0
resources_spreadsheet_view/color_rows=true
resources_spreadsheet_view/array_min_width=128.0
resources_spreadsheet_view/resource_preview_size=32.0
resources_spreadsheet_view/clip_headers=false
resources_spreadsheet_view/dupe_arrays=true
resources_spreadsheet_view/context_menu_on_leftclick=false

[application]

config/name="StormScape"
config/version="0.1"
run/main_scene="res://Stages/World/Game.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/bg_color=Color(0.104389, 0.121748, 0.27417, 1)
config/icon="res://Assets/Icon/icon.svg"

[audio]

buses/default_bus_layout="res://Utilities/Game/Audio/default_bus_layout.tres"

[autoload]

DebugFlags="*res://Utilities/Autoloads/DebugFlags.gd"
SignalBus="*res://Utilities/Autoloads/SignalBus.gd"
AudioManager="*res://Utilities/Autoloads/AudioManager.tscn"
GlobalData="*res://Utilities/Autoloads/GlobalData.gd"
SaverLoader="*res://Utilities/Autoloads/SaverLoader.gd"
RuntimeDebugTools="*res://addons/runtime_debug_tools/scenes/remote_inspector.tscn"
CooldownManager="*res://Utilities/Autoloads/CooldownManager.gd"

[debug]

gdscript/warnings/untyped_declaration=1
gdscript/warnings/inferred_declaration=1

[display]

window/size/viewport_width=480
window/size/viewport_height=270
window/size/window_width_override=3840
window/size/window_height_override=2160
window/stretch/mode="canvas_items"

[editor]

naming/scene_name_casing=1
naming/script_name_casing=1
version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/runtime_debug_tools/plugin.cfg", "res://addons/script-ide/plugin.cfg")

[file_customization]

folder_colors={
"res://Assets/": "red",
"res://Entities/": "green",
"res://Stages/": "orange",
"res://UI/": "blue",
"res://Utilities/": "purple",
"res://Utilities/Common/": "purple",
"res://addons/": "gray"
}

[global_group]

core="Nodes that are core to every scene like the root and the lighting environment."
has_save_logic="A collection of all nodes in the game that must be saved and subsequently loaded."
Items=""

[gui]

theme/custom="res://Assets/Theming/DefaultTheme.tres"
theme/custom_font="res://Assets/Theming/Fonts/GameFontResources/ui_pixel_font.tres"

